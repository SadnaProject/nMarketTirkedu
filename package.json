{
  "name": "n-market-tirkedu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test-dev-server": "vitest --coverage",
    "test:client": "playwright test",
    "dev": "pnpm db-push && run-p dev:*",
    "typecheck": "tsc --noEmit",
    "db-push": "prisma db push && prisma generate",
    "studio": "prisma studio",
    "lint-fix": "eslint --fix ./src && prettier --write ./src",
    "gen-test": "npx playwright codegen localhost:3000",
    "db-seed": "prisma db seed",
    "build": "prisma generate && prisma migrate deploy && run-s build:*",
    "start": "run-p start:*",
    "start:wss": "cross-env PORT=3001 NODE_ENV=production tsx watch src/server/service/wssDevServer.ts --tsconfig tsconfig.server.json",
    "start:next": "next start",
    "dev:next": "next dev",
    "dev:wss": "cross-env PORT=3001 NODE_ENV=development tsx watch src/server/service/wssDevServer.ts --tsconfig tsconfig.server.json",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives src",
    "coverage": "vitest run --coverage",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "test-all-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-all-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "db-migrate-dev": "prisma migrate dev && prisma generate",
    "test": "run-p test:*",
    "test:server": "vitest run",
    "prebuild": "prisma generate && prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@dicebear/collection": "^6.0.3",
    "@dicebear/core": "^6.0.3",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@hookform/resolvers": "^3.1.0",
    "@mantine/hooks": "^6.0.10",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.0",
    "@tanstack/react-query": "^4.29.5",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.12.10",
    "fuzzysearch-ts": "^1.0.3",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "preline": "^1.8.0",
    "ramda": "^0.29.0",
    "rc-slider": "10.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-wrap-balancer": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "superjson": "1.12.2",
    "tailwind-merge": "^1.12.0",
    "tsx": "^3.12.7",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.11.0",
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.33.0",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.9",
    "@types/prettier": "^2.7.2",
    "@types/ramda": "^0.29.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/coverage-istanbul": "^0.29.8",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^40.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.14.0",
    "start-server-and-test": "^1.15.4",
    "tailwind-scrollbar": "^3.0.1",
    "tailwindcss": "^3.3.2",
    "ts-mixer": "^6.0.3",
