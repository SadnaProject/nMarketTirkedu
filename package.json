{
  "name": "n-market-tirkedu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:server": "vitest --coverage",
    "test:client": "playwright test",
    "dev": "pnpm db-push && run-p dev:*",
    "typecheck": "tsc --noEmit",
    "db-push": "prisma db push && prisma generate",
    "studio": "prisma studio",
    "lint-fix": "eslint --fix ./src && prettier --write ./src",
    "gen-test": "npx playwright codegen localhost:3000",
    "db-seed": "prisma db seed",
    "build": "prisma generate && prisma migrate deploy && run-s build:*",
    "start": "run-p start:*",
    "start:wss": "cross-env PORT=3001 NODE_ENV=production tsx src/server/service/wssDevServer.ts --tsconfig tsconfig.server.json",
    "start:next": "next start",
    "dev:wss": "cross-env PORT=3001 NODE_ENV=development tsx watch src/server/service/wssDevServer.ts --tsconfig tsconfig.server.json",
    "dev:next": "next dev",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives src",
    "coverage": "vitest run --coverage",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "test-all-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-all-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "db-migrate-dev": "prisma migrate dev && prisma generate",
    "test": "run-p test:*",
    "prebuild": "prisma generate && prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.13.0",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "dayjs": "^1.11.7",
    "fuzzysearch-ts": "^1.0.3",
    "next": "^13.2.4",
    "next-auth": "^4.21.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "tsx": "^3.12.3",
    "ws": "^8.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.10.0",
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.32.3",
    "@tanstack/react-query-devtools": "^4.28.0",
    "@types/eslint": "^8.21.3",
    "@types/node": "^18.15.5",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^40.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "prisma": "^4.13.0",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.3.0",
    "ts-mixer": "^6.0.3",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  },
  "ct3aMetadata": {
    "initVersion": "7.11.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
